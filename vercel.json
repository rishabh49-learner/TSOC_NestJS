{
    "version": 2,
    "builds":[
        {
            "src": "src/main.ts",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "src/maints",
            "method":[
                "GET",
                "POST",
                "PUT",
                "DELETE"

            ]
        }
    ]
    
}